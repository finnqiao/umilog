{
  "version": 8,
  "name": "UmiLog Underwater Vector",
  "glyphs": "https://protomaps.github.io/basemaps-assets/fonts/{fontstack}/{range}.pbf",
  "sprite": "https://protomaps.github.io/basemaps-assets/sprites/v4/dark",
  "sources": {
    "protomaps": {
      "type": "vector",
      "tiles": [
        "https://api.protomaps.com/tiles/v4/{z}/{x}/{y}.mvt?key=protomaps_public_key"
      ],
      "maxzoom": 15,
      "attribution": "<a href=\"https://protomaps.com\">Protomaps</a> | <a href=\"https://openstreetmap.org\">OSM</a>"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#08141A"
      }
    },
    {
      "id": "earth",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "earth",
      "paint": {
        "fill-color": "#0D1F26"
      }
    },
    {
      "id": "landuse-park",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "landuse",
      "filter": ["any",
        ["==", "pmap:kind", "park"],
        ["==", "pmap:kind", "nature_reserve"],
        ["==", "pmap:kind", "forest"]
      ],
      "paint": {
        "fill-color": "#0A1A1F",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "landuse-urban",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "landuse",
      "filter": ["any",
        ["==", "pmap:kind", "residential"],
        ["==", "pmap:kind", "commercial"],
        ["==", "pmap:kind", "industrial"]
      ],
      "paint": {
        "fill-color": "#101D24",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "water",
      "paint": {
        "fill-color": [
          "interpolate", ["linear"], ["zoom"],
          0, "#0A0F2A",
          5, "#081520",
          10, "#06101A",
          15, "#050D15"
        ]
      }
    },
    {
      "id": "water-outline",
      "type": "line",
      "source": "protomaps",
      "source-layer": "water",
      "paint": {
        "line-color": "#1A3040",
        "line-width": 0.5,
        "line-opacity": 0.4
      }
    },
    {
      "id": "roads-minor",
      "type": "line",
      "source": "protomaps",
      "source-layer": "roads",
      "filter": ["any",
        ["==", "pmap:kind", "minor_road"],
        ["==", "pmap:kind", "other"]
      ],
      "minzoom": 12,
      "paint": {
        "line-color": "#1A2830",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          12, 0.5,
          16, 2
        ],
        "line-opacity": 0.6
      }
    },
    {
      "id": "roads-medium",
      "type": "line",
      "source": "protomaps",
      "source-layer": "roads",
      "filter": ["any",
        ["==", "pmap:kind", "medium_road"]
      ],
      "minzoom": 9,
      "paint": {
        "line-color": "#1F3540",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          9, 0.5,
          12, 1.5,
          16, 4
        ],
        "line-opacity": 0.7
      }
    },
    {
      "id": "roads-major",
      "type": "line",
      "source": "protomaps",
      "source-layer": "roads",
      "filter": ["any",
        ["==", "pmap:kind", "major_road"],
        ["==", "pmap:kind", "highway"]
      ],
      "minzoom": 6,
      "paint": {
        "line-color": "#254050",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          6, 0.5,
          10, 1.5,
          14, 4,
          18, 8
        ],
        "line-opacity": 0.8
      }
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "buildings",
      "minzoom": 13,
      "paint": {
        "fill-color": "#15252E",
        "fill-opacity": [
          "interpolate", ["linear"], ["zoom"],
          13, 0.3,
          16, 0.5
        ]
      }
    },
    {
      "id": "place-labels-minor",
      "type": "symbol",
      "source": "protomaps",
      "source-layer": "places",
      "filter": ["any",
        ["==", "pmap:kind", "neighbourhood"],
        ["==", "pmap:kind", "suburb"]
      ],
      "minzoom": 13,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Noto Sans Regular"],
        "text-size": 11,
        "text-transform": "uppercase",
        "text-letter-spacing": 0.1
      },
      "paint": {
        "text-color": "#4A7080",
        "text-halo-color": "#08141A",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "place-labels-town",
      "type": "symbol",
      "source": "protomaps",
      "source-layer": "places",
      "filter": ["any",
        ["==", "pmap:kind", "village"],
        ["==", "pmap:kind", "town"]
      ],
      "minzoom": 9,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Noto Sans Medium"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          9, 11,
          14, 14
        ]
      },
      "paint": {
        "text-color": "#5A8595",
        "text-halo-color": "#08141A",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-labels-city",
      "type": "symbol",
      "source": "protomaps",
      "source-layer": "places",
      "filter": ["==", "pmap:kind", "city"],
      "minzoom": 5,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Noto Sans Bold"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          5, 12,
          10, 18,
          14, 22
        ]
      },
      "paint": {
        "text-color": "#6A9AAA",
        "text-halo-color": "#08141A",
        "text-halo-width": 2.5
      }
    },
    {
      "id": "place-labels-country",
      "type": "symbol",
      "source": "protomaps",
      "source-layer": "places",
      "filter": ["==", "pmap:kind", "country"],
      "minzoom": 2,
      "maxzoom": 8,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Noto Sans Bold"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          2, 10,
          5, 14,
          8, 18
        ],
        "text-transform": "uppercase",
        "text-letter-spacing": 0.15
      },
      "paint": {
        "text-color": "#4A6A7A",
        "text-halo-color": "#08141A",
        "text-halo-width": 2
      }
    },
    {
      "id": "water-labels",
      "type": "symbol",
      "source": "protomaps",
      "source-layer": "water",
      "filter": ["has", "name"],
      "minzoom": 4,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Noto Sans Italic"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          4, 10,
          8, 14,
          12, 16
        ],
        "symbol-placement": "point"
      },
      "paint": {
        "text-color": "#2A5060",
        "text-halo-color": "#0A0F2A",
        "text-halo-width": 1.5
      }
    }
  ]
}
