<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>UmiLog</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>UmiLog</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.0</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>app.umilog</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>umilog</string>
			</array>
		</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>CFBundleIconName</key>
	<string>AppIcon</string>
	<key>NSFaceIDUsageDescription</key>
	<string>UmiLog uses Face ID to secure your dive logs.</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>Connect your dive computer to sync dive data automatically.</string>
	<key>NSCameraUsageDescription</key>
	<string>Take photos of marine life to attach to wildlife sightings.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>UmiLog can remind you to log a dive when you arrive at or leave a dive site using low-power geofencing.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>UmiLog shows dive sites near you and helps center the map on your location.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>UmiLog uses the microphone for voice input during dive logging.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Select photos to attach to sightings and certification cards.</string>
	<key>NSUserNotificationsUsageDescription</key>
	<string>Get dive reminders at dive sites and alerts for important gear events.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>UmiLog uses voice recognition for hands-free dive logging. All processing happens on your device.</string>
	<key>UILaunchScreen</key>
	<dict>
		<key>UIColorName</key>
		<string>LaunchBackground</string>
	</dict>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>arm64</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
</dict>
</plist>
